<div id="notice">
  <%= notice %>
</div>

<h2>Create User</h2>

<%= form_for(@user) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <br><br>
  <%= f.label :email %>
  <%= f.text_field :email %>
  <br><br>
  <%= f.submit "Create user" %>
<% end %>
  <br>
<%= link_to "Back", users_path %>
