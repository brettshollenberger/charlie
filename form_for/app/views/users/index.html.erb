<div id="notice">
  <%= notice %>
</div>

<table>
  <thead>
    <th>User</th>
    <th>Email</th>
  </thead>
  <tbody>
    <% if @users %>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
        <td><%= link_to "Show", user_path(user) %></td>
        <td><%= link_to "Edit", edit_user_path(user) %></td>
        <td><%= link_to "Destroy", user_path(user), method: :delete %></td>
      </tr>
    <% end %>
    <% end %>
  </tbody>
</table>


<%= link_to "Create User", new_user_path %>



